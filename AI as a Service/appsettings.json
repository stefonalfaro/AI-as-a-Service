{
  //SQL Server
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=AIAsAServiceDb;Trusted_Connection=True;"
  },

  //CosmosDB
  "CosmosDbSettings": {
    "AccountEndpoint": "<your-cosmosdb-account-endpoint>",
    "AccountKey": "<your-cosmosdb-account-key>",
    "DatabaseName": "<your-cosmosdb-database-name>",
    "ContainerName": "<your-cosmosdb-container-name>"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Stripe": {
    "ApiKey": "your_stripe_api_key"
  },
  "OpenAI": {
    "ApiKey": "your_stripe_api_key"
  },
  "JiraApi": {
    "BaseUrl": "https://your-domain.atlassian.net/rest/api/3/",
    "EncodedCredentials": "your_base64_encoded_credentials"
  }
}
